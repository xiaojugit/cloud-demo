/**app.wxss**/

page {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  background: #f4f4f4;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}

button {
  background: initial;
}

button:focus {
  outline: 0;
}

button::after {
  border: none;
}

.text-content {
  word-wrap: break-word;
  word-break: normal;
  white-space: pre-wrap;
}





